<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <SiteUrlToLaunchAfterPublish>http://www.danpickford.com/BiteCard</SiteUrlToLaunchAfterPublish>
    <MSDeployServiceURL>https://w05.winhost.com:8172/MsDeploy.axd</MSDeployServiceURL>
    <DeployIisAppPath>danpickford.com/BiteCard</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <UserName>danpickf</UserName>
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="BiteCards.Classes.Context.BrowsingContext" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="BiteCards.Classes.Context.BrowsingContext, BiteCards" MigrationConfiguration="BiteCards.Migrations.Configuration, BiteCards" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)BiteCards.Classes.Context.BrowsingContext-Web.config Connection String">
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="BiteCards.Classes.Context.BrowsingContext" />
  </ItemGroup>
</Project>